<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.pojo.Plot" table="plot">
<id name="id" column="id"></id>
<property name="plotNo" column="plotNo"></property>
<property name="ownerName" column="ownerName"></property>
<property name="email" column="email"></property>
<property name="phone" column="phone"></property>
<property name="address" column="address"></property>
<property name="amount" column="amount"></property>
<property name="venture" column="venture"></property>
<property name="status" column="status"></property>
<property name="area" column="area"></property>
<property name="filename" column="filename"></property>
</class>
</hibernate-mapping>